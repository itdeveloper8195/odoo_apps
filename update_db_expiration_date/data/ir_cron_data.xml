<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- Scheduler -->
        <record id="ir_cron_update_expiration_date" model="ir.cron">
            <field name="name">Update Expiration Date</field>
            <field name="model_id" ref="base.model_ir_config_parameter"/>
            <field name="state">code</field>
            <field name="code">model.update_expiration_date()</field>
            <field name="user_id" ref="base.user_root"/>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="numbercall">-1</field>
            <field name="active">True</field>
        </record>
    </data>
</odoo>
